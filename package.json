{
  "name": "@cto.af/eslint-config",
  "version": "1.0.4",
  "description": "hildjj's lint rules",
  "main": "index.js",
  "scripts": {
    "test": "eslint -c index.js *.js",
    "lint": "eslint . --ext js,cjs,mjs",
    "unused": "eslint-find-rules index.js -u && eslint-find-rules ava.js -u --no-core && eslint-find-rules jsdoc.js -u --no-core && eslint-find-rules ts.js -u --no-core"
  },
  "keywords": [
    "lint"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": "hildjj/ctoaf-eslint-config",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~5",
    "@typescript-eslint/parser": "~5",
    "eslint-plugin-ava": "~14",
    "eslint-plugin-jsdoc": "~40",
    "eslint-plugin-node": "~11",
    "typescript": "~4"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "eslint-plugin-ava": {
      "optional": true
    },
    "eslint-plugin-jsdoc": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "^8.34.0",
    "eslint-find-rules": "4.1.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-hildjj": "link:rules",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-node": "^11.1.0",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
